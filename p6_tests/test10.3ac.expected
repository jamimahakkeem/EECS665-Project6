[BEGIN GLOBALS]
[END GLOBALS]
[BEGIN func LOCALS]
a (local var of 8 bytes)
tmp0 (tmp var of 8 bytes)
[END func LOCALS]
fun_func:   enter func
            RECEIVE [a]
            [tmp0] := [a] EQ64 0
            IFZ [tmp0] GOTO lbl_1
            [a] := [a] ADD64 1
lbl_1:      nop
lbl_0:      leave func
